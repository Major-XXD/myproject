def generic_response(q):
    return f"""\033[95m
No preset for '{q}'.
But here's how to get the answer:
- Use GitHub/Google/CTF writeups for '{q}'.
- Try 'pkg install' or 'pip install' for tools.
- Clone repos: git clone <url>
- Ask again with more detail for a step-by-step!
- Want a script, exploit, code, or tool? Just ask for it by name or function!
\033[0m"""

def main():
    clear()
    print(BANNER)
    print("Type 'help' for examples, or ask ANY question (step-by-step, code, hacking, dev, etc).")
    while True:
        try:
            q = input("\033[91mCYBER-GPT>\033[0m ").strip().lower()
            if not q: continue
            if q in ["exit", "quit"]:
                print("ðŸ’€ SESSION TERMINATED."); break
            found = False
            for k in KNOWLEDGE:
                if q == k or k in q:
                    print(KNOWLEDGE[k])
                    found = True
                    break
            if not found:
                print(generic_response(q))
        except KeyboardInterrupt:
            print("\nðŸ’€ FORCE EXIT"); break
        except Exception as e:
            print(f"Error: {e}")

if __name__ == "__main__":
    main()
